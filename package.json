{
  "name": "copy-webpack-plugin",
  "version": "4.6.0",
  "description": "Copy files && directories with webpack",
  "author": "Len Boyette",
  "license": "MIT",
  "main": "dist/index.js",
  "engines": {
    "node": ">= 6.9.0"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "npm run build -- -w",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production babel src -d dist --ignore 'src/**/*.test.js' --copy-files",
    "clean": "del-cli dist",
    "lint": "eslint src/ tests/",
    "prepublish": "npm run build",
    "release": "standard-version",
    "test:only": "npm run build && npm run build:tests && mocha compiled_tests/",
    "pretest": "npm run lint",
    "test": "npm run test:only",
    "build:tests": "babel tests -d compiled_tests && rimraf compiled_tests/helpers && ncp tests/helpers compiled_tests/helpers && node scripts/createSpecialDirectory.js"
  },
  "dependencies": {
    "globby": "^7.1.1",
    "cacache": "^10.0.4",
    "find-cache-dir": "^1.0.0",
    "serialize-javascript": "^1.4.0",
    "is-glob": "^4.0.0",
    "loader-utils": "^1.1.0",
    "minimatch": "^3.0.4",
    "p-limit": "^1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@webpack-contrib/eslint-config-webpack": "^3.0.0",
    "cross-env": "^5.1.3",
    "del-cli": "^1.1.0",
    "chai": "^4.2.0",
    "enhanced-resolve": "^3.4.1",
    "eslint": "^5.13.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-prettier": "^3.0.0",
    "is-gzip": "^2.0.0",
    "mkdirp": "^0.5.1",
    "mocha": "^5.2.0",
    "ncp": "^2.0.0",
    "rimraf": "^2.6.2",
    "prettier": "^1.14.0",
    "standard-version": "^5.0.0"
  },
  "homepage": "https://github.com/webpack-contrib/copy-webpack-plugin",
  "bugs": "https://github.com/webpack-contrib/copy-webpack-plugin/issues",
  "repository": "https://github.com/webpack-contrib/copy-webpack-plugin.git",
  "keywords": [
    "webpack",
    "plugin",
    "transfer",
    "move",
    "copy"
  ],
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "6.9.0"
          },
          "useBuiltIns": "usage"
        }
      ]
    ]
  }
}
